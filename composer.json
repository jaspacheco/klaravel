{
    "name": "ksoft/klaravel",
    "version": "1.0.9",
    "description": "This package provides functionalities and structures definitions that are able to speed up your development generating classes and repositories with extended functionality that allows implementation of advanced techniques used in large projects where maintainability its key.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiko Seijo",
            "email": "kiko@sunnyface.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ksoft\\Klaravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "doctrine/dbal": "^2.6",
        "kevupton/ethereal": "v2.3.9.6",
        "zircote/swagger-php": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ksoft\\Klaravel\\ServiceProvider"
            ],
            "aliases": {
                "Larapp": "Ksoft\\Klaravel\\Facade"
            }
        }
    },
    "require-dev": {
        "laravel/laravel": "^5.5",
        "laravel/lumen": "^5.5"
    }
}
