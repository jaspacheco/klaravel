{
  "name": "ksoft/klaravel",
  "description":
    "Development helper to speed up development, backups, logs, crud, swagger, repository, graphql...",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kiko Seijo",
      "email": "kiko@sunnyface.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ksoft\\Klaravel\\": "src/"
    },
    "files": ["src/helpers.php"]
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "require": {
    "anahkiasen/former": "^4.1",
    "doctrine/dbal": "^2.6",
    "jenssegers/date": "^3.2",
    "spatie/laravel-activitylog": "^2.5",
    "spatie/laravel-backup": "^5.2",
    "zircote/swagger-php": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": ["Ksoft\\Klaravel\\ServiceProvider"],
      "aliases": {
        "Larapp": "Ksoft\\Klaravel\\Facade"
      }
    }
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "laravel/laravel": "5.6.*",
    "laravel/lumen": "5.6.*"
  }
}
